{
  "plugins": ["prettier-plugin-organize-attributes"],
  "attributeGroups": [
    "^#.*",
    "$ANGULAR_STRUCTURAL_DIRECTIVE",
    "$ANGULAR_ANIMATION",
    "$ANGULAR_INPUT",
    "$ANGULAR_OUTPUT",
    "^id$",
    "^class$",
    "$DEFAULT"
  ],
  "attributeSort": "ASC",
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "rangeStart": 0,
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto"
}
